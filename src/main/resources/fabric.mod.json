{
  "schemaVersion": 1,
  "id": "offhandoverhaul",
  "version": "${version}",
  "name": "Offhand Overhaul",
  "description": "Stops block being placed from the off-hand slot under certain, configurable conditions",
  "authors": [
    "Jamalam"
  ],
  "contact": {
    "sources": "https://github.com/JamCoreModding/OffhandOverhaul",
    "issues": "https://github.com/JamCoreModding/OffhandOverhaul/issues"
  },
  "license": "MIT",
  "icon": "assets/offhandoverhaul/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": [
      "io.github.jamalam360.offhandoverhaul.OffhandOverhaulModInit"
    ],
    "modmenu": [
      "io.github.jamalam360.offhandoverhaul.config.ModMenuIntegration"
    ]
  },
  "mixins": [
    "offhandoverhaul.mixins.json"
  ],
  "depends": {
    "fabricloader": ">=0.12.5",
    "fabric": "*",
    "jamlib": ">=0.0.5"
  },
  "suggests": {
    "modmenu": "*"
  },
  "custom": {
    "notify_gradle_properties_url": "https://raw.githubusercontent.com/JamCoreModding/OffhandOverhaul/main/gradle.properties",
    "notify_gradle_properties_key": "mod_version"
  }
}
